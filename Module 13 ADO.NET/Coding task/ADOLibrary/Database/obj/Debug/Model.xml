		<Element Type="SqlProcedure" Name="[dbo].[spBulkDelete]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
	DECLARE @Count INT
	Declare @for_delete INT
	Declare @chunk_size INT

	SELECT @chunk_size=1
	SELECT @Count = 0
	select @for_delete=count(*) from [Order] where ([Status] = @Status or @Status is null) AND (MONTH([CreatedDate]) = @Month or @Month is null) AND (YEAR([CreatedDate]) = @Year or @Year is null)
	While (@Count < @for_delete)
	BEGIN
		SELECT @Count = @Count + @chunk_size
		BEGIN TRAN
			DELETE top(@chunk_size) from [Order] where ([Status] = @Status or @Status is null) AND (MONTH([CreatedDate]) = @Month or @Month is null) AND (YEAR([CreatedDate]) = @Year or @Year is null)
		COMMIT TRAN
	END
END]]></Value>
			</Property>
